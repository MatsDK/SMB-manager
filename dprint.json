{
	"indentWidth": 4,
	"typescript": {
		"quoteStyle": "preferSingle",
		"semiColons": "asi"
	},
	"prettier": {
		"singleQuote": true,
		"associations": [
			"**/*.yaml",
			"**/*.json5"
		]
	},
	"rustfmt": {
		"tab_spaces": 4
	},
	"includes": [
		"src-tauri/src/**/*.{rs}",
		"src/**/*.{ts,tsx,js,jsx,cjs,mjs,json,json5,yaml}",
		"server/**/*.{ts,tsx,js,jsx,cjs,mjs,json,json5,yaml}"
	],
	"excludes": [
		"**/node_modules",
		"pnpm-lock.yaml",
		"dist",
		".next"
	],
	"plugins": [
		"https://plugins.dprint.dev/typescript-0.67.0.wasm",
		"https://plugins.dprint.dev/json-0.15.1.wasm",
		"https://plugins.dprint.dev/markdown-0.13.1.wasm",
		"https://plugins.dprint.dev/prettier-0.6.1.exe-plugin@2d54d5f8dbdeb087762fadc4840c688327f5deab447c94b32ca3850106db9224",
		"https://plugins.dprint.dev/rustfmt-0.6.2.json@886c6f3161cf020c2d75160262b0f56d74a521e05cfb91ec4f956650c8ca76ca"
	]
}